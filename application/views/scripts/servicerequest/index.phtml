   {
	   $('#ticket_done').removeClass('openpop');
	  // window.location.href=("<?php echo BASEPATH; ?>/Servicerequestlist");
	   window.location.href=("<?php echo BASEPATH; ?>/Servicerequest");
	   
   }
   function validateEmail(email) {
	    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	    return re.test(email);
	}
	
  function phonenumber(mob)
   {
       var patte1 = new RegExp(/^\d{10}$/);
   	   return patte1.test(mob);
   }

      
