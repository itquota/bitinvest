   function generateTicket()
   {
		$('#error').hide();
		$('#btnGroup').hide();
		var language=$('#language').val();
		var category=$('#category').val();
		var comment=$('#comment').val();
		var token=$('#token').val();
		var mob = $('#mobile').val();
		var email = $('#email').val();
	   	var txn = $('#txn').val();
	    var invoice = $('#invoice').val();
	    var payment_id = $('#payment_id').val();
	    var mode = $('#mode').val();
	//	alert(payment_id);
	   
	   if(!language || language=='select')
		{
			$('#error').empty();
			$('#error').append('Please Select Language');
			$('#error').css("display","block");
			$('#btnGroup').show();
			
			return false;
		}else if(!category || category=='select')
		{
			$('#error').empty();
			$('#error').append('Service Request Category');
			$('#error').css("display","block");
			$('#btnGroup').show();			
			return false;
		}
		else if(!email || email=='')
		{
			
			$('#error').empty();
			$('#error').append('Please Add Email');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
		}
		else if(!validateEmail(email))
		{
			$('#error').empty();
			$('#error').append('Please Enter valid Email address');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
		}
		
		else if(!mob || mob=='')
		{	
			$('#error').empty();
			$('#error').append('Please Add Mobile');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
		}
		else if(!phonenumber(mob))
		{	
			$('#error').empty();
			$('#error').append('Please enter valid Mobile Number');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
		}
		
   
	   	else if(category=='Kit Activation' && invoice=='')
		{
			$('#error').empty();
			$('#error').append('Invoice ID is required');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
		}
	   	else if(category=='Kit Activation' && txn=='')
		{
			$('#error').empty();
			$('#error').append('Transaction number is required');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
			
		}	   
	   	/*else if(mode=='2' && payment_id=='')
		{
			$('#error').empty();
			$('#error').append('Payment/Order ID required');
			$('#error').css("display","block");
			return false;
		}*/			
		else if(!comment || comment=='')
		{
			$('#error').empty();
			$('#error').append('Please Add Description');
			$('#error').css("display","block");
			$('#btnGroup').show();
			return false;
		} 		
		else
		{
			 $.ajax({
					
				//	url:"<?php echo BASEPATH; ?>/Servicerequest/generateticket", 
					url:"<?php echo BASEPATH; ?>/Servicerequest/generateticketviaapi", 
					type: "POST",    
				    data: 'payment_id='+payment_id+'&type='+mode+'&txn='+txn+'&category='+category+'&comment='+comment+'&token='+token+'&mob='+mob+'&email='+email+'&language='+language+'&invoice='+invoice,
					
			  		success:function(response)
			  		{			  
						var data=JSON.parse(response);
						if(data['success']!='')
						{  //alert(data);
							$('#cont_hold').empty();
						//	$('#cont_hold').append('<b>'+data['data']+'</b>');
							$('#cont_hold').append('<b>Your Ticket Has Been Raised, We\'\ll Get Back To You Soon.</b>');
							$('#ticket_done').addClass('openpop');
							$('#ticket_done').addClass('alert');
							$('#ticket_done').addClass('alert-success');
							setTimeout(function(){
							 $('#ticket_done').removeClass('openpop');
							// window.location.href=("<?php echo BASEPATH; ?>/Servicerequestlist");
							 window.location.href=("<?php echo BASEPATH; ?>/Servicerequest");
							},5000);
							$('#btnGroup').show();							
							return false;
						}
						else
						{
							$('#error').empty();
							$('#error').append(data['data']);
							$('#error').css("display","block");
							$('#btnGroup').show();							
							return false;
						}
					//	$('#btnGroup').show();

					}
				 });
		}
   }
